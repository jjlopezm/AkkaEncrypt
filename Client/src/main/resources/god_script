 #Generate SelfSignedCA
 openssl genrsa -out ca.key 1024
 openssl req -config crt.conf -new -key ca.key -out ca.csr
 openssl x509 -req -days 1024 -in ca.csr -signkey ca.key -out ca.crt

 #Generate Cert
 openssl genrsa -out gosec-sso.key 1024
 openssl req -new  -config crt.conf -key gosec-sso.key -out gosec-sso.csr
 openssl x509 -req -days 1024 -CAcreateserial -CAserial ca.seq -in gosec-sso.csr -CA ca.crt -CAkey ca.key -out gosec-sso.crt

 #Generate keystore
 cat gosec-sso.crt ca.crt > cert-chain.pem
 openssl pkcs12 -password pass:stratio -export -inkey gosec-sso.key -in cert-chain.pem -out gosec-sso.pkcs12
 keytool -noprompt -importkeystore -srckeystore gosec-sso.pkcs12 -srcstorepass stratio -deststorepass stratio -srcstoretype PKCS12 -destkeystore gosec-sso-keystore
